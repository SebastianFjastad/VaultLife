        <th>
            @Html.DisplayNameFor(model => model.AgeBand)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Territory.TerritoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberSubscriptionType.MemberSubscriptionTypeCode)
        </th>
        <th></th>
