@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Game.GameCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Country.CountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State.StateName)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.CityID)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.AgeBand)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Territory.TerritoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberSubscriptionType.MemberSubscriptionTypeCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.GameMemberFilter.GameMemberFilterID }) |
            @Html.ActionLink("Details", "Details", new { id = item.GameMemberFilter.GameMemberFilterID }) |
